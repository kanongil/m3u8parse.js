{
  "name": "m3u8parse",
  "version": "1.1.1",
  "description": "Structural parsing of Apple HTTP Live Streaming .m3u8 format",
  "main": "m3u8parse.js",
  "scripts": {
    "test": "lab -t 90 -L",
    "test-cov-html": "lab -a @hapi/code -r html -o coverage.html",
    "coverage": "istanbul cover _mocha -- --ui bdd -R spec -t 1000",
    "coveralls": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls"
  },
  "keywords": [
    "hls",
    "playlist",
    "video",
    "audio"
  ],
  "author": "Gil Pedersen <gpdev@gpost.dk>",
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "clone": "^1.0.1",
    "cuint": "^0.2.0",
    "split": "^1.0.0"
  },
  "devDependencies": {
    "@hapi/code": "^5.3.1",
    "@hapi/lab": "^18.1.2",
    "readable-stream": "^3.3.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kanongil/node-m3u8parse.git"
  },
  "bugs": {
    "url": "https://github.com/kanongil/node-m3u8parse/issues"
  }
}
