    "split": "^1.0.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "collect-stream": "^1.2.1",
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.1",
    "mocha": "^2.2.1",
