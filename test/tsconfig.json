{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "target": "ES2020",
    "lib": [ "ES2020" ],
    "module": "ES2020",
    "moduleResolution": "node",
    "isolatedModules": true,
    "declaration": false,
    "strict": true,
    "strictNullChecks": true,
    "esModuleInterop": false,
    "types": ["node"],
    "inlineSourceMap": true,
    "inlineSources": true
  },
  "references": [{
    "path": "../src/tsconfig.debug.json",
  }],
  "include": ["*.ts"]
}
